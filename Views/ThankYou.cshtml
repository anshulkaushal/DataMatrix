@using Umbraco.Cms.Core.Models.Blocks
@using Umbraco.Cms.Web.Common.PublishedModels;
@using Umbraco.Cms.Core.Models
@using Umbraco.Cms.Core.Models.PublishedContent
@using System.Text
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.ThankYou>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = null;

     // Thank You   
       var logo = Model.Value<Umbraco.Cms.Core.Models.MediaWithCrops>("logo");
     var message = Model.Value<string>("message");
     var homeCtaText = Model.Value<string>("homeCtaText");
     var homeCtaLink = Model.Value<IEnumerable<Link>>("homeCtaLink");
     var homeLink = homeCtaLink?.FirstOrDefault();
     var mediaLinkText = Model.Value<string>("mediaLinkText");
     var mediaLinks = Model.Value<string>("mediaLinks");

}
<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <meta name="description" content="Vyauma AI - Empowering design, collaboration, and innovation through AI." />
  <title>Thank You</title>

  <link rel="preload" href="/css/style.css" as="style" />
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" />
 <link href="~/css/style.css" rel="stylesheet">

  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css" />
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick-theme.css" />

 <link href="/media/n3pllfsp/fav-icon.svg" rel="icon">
  
</head>

<body>
  <div class="thankyou-page mt-5">
  <div class="container text-center">
    <div class="row justify-content-center">
      <div class="col">

        <!-- ICON -->
        @if (logo != null)
         {
        <div class="icon-circle mx-auto mb-4">            
          <img src="@logo.Url()"
               class="img-fluid"
               alt="logo" loading="lazy">            
        </div>
        }

        <!-- TITLE -->
        <div>
        @Html.Raw(message.ToString().Replace("<p>", "").Replace("</p>", ""))                
        </div>
        <!-- BUTTON -->
       
        
        

        

        <a href="@homeLink.Url" class="btn btn-outline-dark rounded-pill px-4 py-2 fw-semibold">        
         @homeCtaText
        </a>
        <!-- SOCIAL -->
        <p class="thankyou-social-title">
          @mediaLinkText
        </p>
        
             @Html.Raw(mediaLinks.ToString().Replace("<p>", "").Replace("</p>", ""))            
        

      </div>
    </div>
  </div>
</div>

</body>

</html>