{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "c7ba126a-bcfb-45ce-b25a-d16ae9d8ee06",
        "SanitizeTinyMce": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "EmailTemplates": {
        "ContactInquiry": "<h3>Contact Info </h3><p><strong>Full Name:</strong> {{FullName}}</p><p><strong>Email:</strong> {{Email}}</p><p><strong>Subject:</strong> {{Subject}}</p><p><strong>Message:</strong>{{Message}}</p>",
        "ThankYouMail": "<p>Dear {{FullName}},</p><p>Thank you for reaching out to us. We truly appreciate you taking the time to contact <strong>DataMatrix</strong>.</p><p>We’ve received your message and our team is currently reviewing it. One of our experts will get back to you shortly with the information you need.</p><p>If you have any additional details to share in the meantime, feel free to reply to this email. We’re always happy to help.</p><p>Best regards,<br><strong>DataMatrix Team</strong></p>,"
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Security": {
        "AllowConcurrentLogins": false
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|/DataMatrix.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite"
  }
}
